<?xml version = '1.0' encoding = 'windows-1252'?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">
  <servlet>
    <servlet-name>Faces Servlet</servlet-name>
    <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>FileReceiver</servlet-name>
    <servlet-class>view.FileReceiver</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FileGenerator</servlet-name>
    <servlet-class>view.FileGenerator</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Faces Servlet</servlet-name>
    <url-pattern>/faces/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileReceiver</servlet-name>
    <url-pattern>/filereceiver</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileGenerator</servlet-name>
    <url-pattern>/filegenerator</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>/main.html</welcome-file>
  </welcome-file-list>
</web-app>
